{
  "name": "seasx/seaslogger",
  "description": "An effective,fast,stable log package for PHP",
  "keywords": [
    "log",
    "logging",
    "psr-3"
  ],
  "type": "library",
  "license": "PHP",
  "authors": [
    {
      "name": "SeasX",
      "homepage": "https://github.com/SeasX"
    }
  ],
  "require": {
    "php": "^7.0",
    "psr/log": "^1.0.2",
    "ext-SeasLog": "^1.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  },
  "autoload": {
    "psr-4": {
      "SeasX\\Seaslogger\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SeasX\\Seaslogger\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ]
  }
}
